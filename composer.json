{
    "name": "cwp/cwp-installer",
    "description": "CWP Project Template",
    "license": "BSD-3-Clause",
    "require": {
        "cwp/cwp-recipe-basic": "~1.6.1@stable",
        "cwp/cwp-recipe-blog": "~1.6.1@stable",
        "cwp/starter-theme": "~1.0.0@stable"
    },
    "require-dev": {
        "cwp/cwp-recipe-basic-dev": "^1.1@stable"
    },
    "suggest": {
        "silverstripe/textextraction": "Required for document text searching within CWP"
    },
    "config": {
        "process-timeout": 900
    },
    "extra": [],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.cwp.govt.nz/"
        },
        {
            "type": "vcs",
            "url": "git@github.com:silverstripe-security/silverstripe-multivaluefield"
        },
        {
            "type": "vcs",
            "url": "https://github.com/silverstripe-security/silverstripe-cms.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/silverstripe-security/silverstripe-framework.git"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev"
}