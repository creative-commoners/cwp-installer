{
    "name": "cwp/cwp-installer",
    "description": "CWP Project Template",
    "license": "BSD-3-Clause",
    "require": {
        "cwp/cwp-recipe-basic": "~1.6.2@stable",
        "cwp/cwp-recipe-blog": "~1.6.2@stable",
        "cwp/starter-theme": "~1.1.1@stable"
    },
    "require-dev": {
        "cwp/cwp-recipe-basic-dev": "^1.1@stable"
    },
    "suggest": {
        "silverstripe/textextraction": "Required for document text searching within CWP"
    },
    "config": {
        "process-timeout": 900
    },
    "extra": [],
    "repositories": [
      {
        "type": "package",
        "package": {
          "name": "silverstripe/multivaluefield",
          "type": "silverstripe-module",
          "version": "2.3.4",
          "dist": {
            "url": "https://github.com/symbiote/silverstripe-multivaluefield/archive/2.3.4.zip",
            "type": "zip"
          }
        }
      }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev"
}
